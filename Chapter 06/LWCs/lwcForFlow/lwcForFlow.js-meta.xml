<?xml version="1.0" encoding="UTF-8"?>
<LightningComponentBundle xmlns="http://soap.sforce.com/2006/04/metadata">
  <apiVersion>57.0</apiVersion>
  <isExposed>true</isExposed>
  <masterLabel>LWC For Flow</masterLabel>
  <description>It shows the data of the contact, which is entered by the user.</description>
  <targets>
    <target>lightning__FlowScreen</target>
  </targets>
  <targetConfigs>
    <targetConfig targets="lightning__FlowScreen">
      <property name="accName" label="Account Chosen" type="String" role="inputOnly" />
      <property name="firstname" label="Contact First Name" type="String" role="inputOnly"/>
      <property name="lastname" label="Contact Last Name" type="String" role="inputOnly" />
      <property name="phone" label="Contact Phone" type="String" role="inputOnly" />
      <property name="email" label="Contact eMail" type="String" role="inputOnly" />
    </targetConfig>
  </targetConfigs>
</LightningComponentBundle>